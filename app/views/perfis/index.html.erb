<p id="notice"><%= notice %></p>

<h1>Perfis</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Nome</th>
      <th>Sobrenome</th>
      <th>Cpf</th>
      <th>Celular</th>
      <th>Data nascimento</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @perfis.each do |perfil| %>
      <tr>
        <td><%= perfil.usuario %></td>
        <td><%= perfil.nome %></td>
        <td><%= perfil.sobrenome %></td>
        <td><%= perfil.cpf %></td>
        <td><%= perfil.celular %></td>
        <td><%= perfil.data_nascimento %></td>
        <td><%= perfil.tipo %></td>
        <td><%= link_to 'Show', perfil %></td>
        <td><%= link_to 'Edit', edit_perfil_path(perfil) %></td>
        <td><%= link_to 'Destroy', perfil, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Perfil', new_perfil_path %>
